     penjualan.`Total_Harga` AS penjualan_Total_Harga,
     penjualan.`Tanggal_Transaksi` AS penjualan_Tanggal_Transaksi,
     penjualan.`Tunai` AS penjualan_Tunai,
     penjualan.`Kembali` AS penjualan_Kembali
FROM
     `penjualan` penjualan
WHERE
     Kode_Transaksi = $P{kode}]]>
