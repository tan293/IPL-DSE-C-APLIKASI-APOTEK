		<![CDATA[SELECT
     *,
     penjualan.`Kode_Transaksi` AS penjualan_Kode_Transaksi,
     penjualan.`Nama_Pembeli` AS penjualan_Nama_Pembeli,
     penjualan.`Kode_Obat` AS penjualan_Kode_Obat,
